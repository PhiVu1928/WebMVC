
@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Bán Vợt </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/x-icon" href="~/Assets/Client/img/favicon.ico">

    <!-- CSS here -->
    <link rel="stylesheet" href="~/Assets/Client/Style/bootstrap.min.css">
    <link rel="stylesheet" href="~/Assets/Client/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Assets/Client/css/flaticon.css">
    <link rel="stylesheet" href="~/Assets/Client/css/slicknav.css">
    <link rel="stylesheet" href="~/Assets/Client/css/animate.min.css">
    <link rel="stylesheet" href="~/Assets/Client/css/magnific-popup.css">
    <link rel="stylesheet" href="~/Assets/Client/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/Assets/Client/css/themify-icons.css">
    <link rel="stylesheet" href="~/Assets/Client/css/slick.css">
    <link href="~/Assets/Client/css/jquery-ui.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Assets/Client/css/nice-select.css">
    <link rel="stylesheet" href="~/Assets/Client/css/style.css" />
    <link rel="stylesheet" href="~/Assets/Client/Style/main.css">

</head>
<body>
    <header>
        @{ Html.RenderAction("MenuHeader", "Home");}
    </header>
    <main>
        @RenderSection("Slider", required: false)
        <!-- Trending Area Start -->
        <section class="whats-news-area pt-50 pb-20 ">
            <div class="container">
                <div class="row">
                    @RenderSection("LeftMenu", required: false)
                    @RenderBody()
                </div>
            </div>
        </section>
        <!-- Trending Area End -->
    </main>
    <footer>
        @{ Html.RenderAction("Footer", "Home");}
    </footer>
    @{ Html.RenderAction("StickyCart", "Home");}
    @{ Html.RenderAction("Search", "Home");}
</body>
</html>
<!-- JS here -->

<script src="~/Assets/Client/js/vendor/modernizr-3.5.0.min.js"></script>
<!-- Jquery, Popper, Bootstrap -->
<script src="~/Assets/Client/js/vendor/jquery-1.12.4.min.js"></script>
